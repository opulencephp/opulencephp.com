{
  "name": "rdev/website",
  "description": "The RDev flagship framework",
  "authors": [
    {
      "name": "David Young",
      "email": "dbyoung2@gmail.com",
      "role": "Architect"
    }
  ],
  "autoload": {
    "psr-4": {
      "Project\\": [
        "app/project",
        "tests/app/project"
      ]
    }
  },
  "require": {
    "php": ">=5.5.10",
    "rdev/rdev": "0.2.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "4.2.*",
    "phpunit/php-file-iterator": ">=1.3.0",
    "phpunit/php-token-stream": ">=1.1.3",
    "phpunit/php-text-template": ">=1.2.0",
    "phpunit/php-invoker": ">=1.1.0",
    "phpunit/phpunit-selenium": ">=1.2",
    "phpunit/phpunit-story": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  },
  "minimum-stability": "dev"
}
